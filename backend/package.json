{
  "name": "twogether-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend para la app de parejas Twogether",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "db:sync": "node -e \"import('./config/db.js').then(m => m.syncDatabase({alter: true}))\"",
    "db:reset": "node -e \"import('./config/db.js').then(m => m.syncDatabase({force: true}))\""
  },
  "keywords": [
    "twogether",
    "couples",
    "nodejs",
    "express",
    "sequelize"
  ],
  "author": "CesarYNadia",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
